//Table of Integer
import java.util.*;
public class Table {
     public static void main(String[] args)
     {
        int i=1,N,m;
        Scanner sc = new Scanner (System.in);
        N=sc.nextInt();
        while(i<=10)
        {
            m=N*i;
            System.out.println(N+"x"+i+"="+m);
            i++;
        }
        
    }
    
}
